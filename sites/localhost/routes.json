[
		{
				"matches": ["/"],
				"type" : "text",
				"stylesheet" : "test.xsl",
				"apps" : [
						{
								"app" : "user",
								"handler" : "user",
								"permissions" : ["public.permission"],
								"cache" : 3600
						}
				],
				"sync": false
		},
		{
			"matches": ["/api/images"],
			"type" : "json",
			"stylesheet" : "test.xsl",
			"apps" : [
					{
						"app" : "generic",
						"handler" : "files",
						"permissions" : ["public.permission"],
						"cache" : 0
					},
					{
						"app" : "user",
						"handler" : "user",
						"permissions" : ["public.permission"],
						"cache" : 0
					}
				],
				"sync": false,
				"settings": {"upload_home": "public/sites", "upload_dir": "images"}
		},
		{
			"matches": ["/api/roles", "/api/roles/*" ],
			"type": "json",
			"apps" : [
				{
					"app": "generic",
					"handler": "records",
					"permissions": ["public.permission"],
					"cache": 10
				}
			],
			"sync": true
		}
]
